{
  "name": "Find all MIs with an Entra ID role",
  "category": "Azure - Managed Identities",
  "queryList": [
      {
          "final": true,
          "query": "MATCH p = (:AZServicePrincipal {serviceprincipaltype: 'ManagedIdentity'})-[:AZHasRole]->(:AZRole) RETURN p",
          "allowCollapse": true
      }
  ]
}
